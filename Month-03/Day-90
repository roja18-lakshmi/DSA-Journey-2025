// https://www.geeksforgeeks.org/problems/find-xor-of-numbers-from-l-to-r/1
class Solution {
    public static int findXOR(int l, int r) {
        return fun(l-1)^fun(r);
    }
    public static int fun(int n){
        if(n%4==0){
            return n;
        }
        if(n%4==1){
            return 1;
        }
        if(n%4==2){
            return n+1;
        }
        return 0;
    }
}