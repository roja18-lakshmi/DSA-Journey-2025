class Solution {
    public int maxProfit(int[] prices) {
        int max=0;
        int best=prices[0];
        for(int i=1;i<prices.length;i++){
            int temp=prices[i]-best;
            if(temp>max){
                max=temp;
            }
            if(prices[i]<best){
                best=prices[i];
            }
        }
        return max;
    }
}